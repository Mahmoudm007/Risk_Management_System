   <string notr="true">QToolTip {
    border: 1px solid black;
    background-color: #ffd37f;
    padding: 4px;
    border-radius: 4px;
    color: #000000;
}

/* Global Widget Settings */
QWidget {
    color: #2e2e2e;
    background-color: #f9f9f9;
}

/* Views */
QTreeView, QListView {
    background-color: #ffffff;
    color: #2e2e2e;
    margin-left: 5px;
}

QWidget:item:hover {
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #ffe0b2, stop:1 #ffc107);
    color: #000000;
}

QWidget:item:selected {
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #ffca28, stop:1 #ff9800);
    color: #000000;
}

/* Disabled Widgets */
QWidget:disabled {
    color: #aaaaaa;
    background-color: #e0e0e0;
}

/* Font Bold for QLabel and QComboBox */
QLabel {
    font-size: 15px;
    font-weight: bold;
}

QComboBox {
    font-weight: bold;
}

/* Line Edit */
QLineEdit {
    background-color: #ffffff;
    padding: 4px;
    height: 30px;
    border: 1px solid #cccccc;
    border-radius: 6px;
    font-size: 18px;
    color: #2e2e2e;
}

/* Push Button */
QPushButton {
    color: #ffffff;
    background-color: #1976d2;
    border: none;
    border-radius: 6px;
    padding: 6px 10px;
    font-size: 15px;
}

QPushButton:hover {
    background-color: #1565c0;
}

QPushButton:pressed {
    background-color: #0d47a1;
}

/* ComboBox */
QComboBox {
    background-color: #ffffff;
    border: 1px solid #cccccc;
    border-radius: 6px;
    height: 30px;
    padding-left: 5px;
}

QComboBox:hover {
    border: 1px solid #1976d2;
}

/* ComboBox Dropdown */
QComboBox::drop-down {
    width: 20px;
    border-left: 1px solid #cccccc;
}

QComboBox QAbstractItemView {
    background-color: #ffffff;
    selection-background-color: #ffc107;
}

/* Scrollbar - Wider and More Contrasted */
QScrollBar:vertical, QScrollBar:horizontal {
    background: #e0e0e0;
    width: 14px;
    height: 14px;
    border: none;
}

QScrollBar::handle:vertical, QScrollBar::handle:horizontal {
    background: #ffc107;
    border-radius: 6px;
    min-height: 20px;
    min-width: 20px;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical,
QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
    background: #1976d2;
    border-radius: 6px;
    height: 14px;
    width: 14px;
}

QScrollBar::up-arrow, QScrollBar::down-arrow,
QScrollBar::left-arrow, QScrollBar::right-arrow {
    background: #ffffff;
    width: 8px;
    height: 8px;
}

/* Text Editors */
QTextEdit, QPlainTextEdit {
    background-color: #ffffff;
    color: #2e2e2e;
}

/* Group Boxes */
QGroupBox {
    border: 1px solid #cccccc;
    margin-top: 10px;
    font-weight: bold;
    color: #2e2e2e;
}

/* Headers */
QHeaderView::section {
    background-color: #f1f1f1;
    color: #2e2e2e;
    border: 1px solid #d0d0d0;
    padding-left: 6px;
}

/* Tabs */
QTabBar::tab {
    color: #2e2e2e;
    background-color: #f5f5f5;
    border: 1px solid #cccccc;
    padding: 6px;
}

QTabBar::tab:selected {
    background-color: #ffffff;
    border-bottom: none;
}

/* Progress Bar */
QProgressBar {
    border: 1px solid #ccc;
    border-radius: 4px;
    text-align: center;
    background-color: #f5f5f5;
}

QProgressBar::chunk {
    background-color: #ffc107;
    width: 10px;
}

/* Sliders */
QSlider::groove:horizontal, QSlider::groove:vertical {
    background: #ccc;
    border-radius: 4px;
}

QSlider::handle:horizontal, QSlider::handle:vertical {
    background: #ffc107;
    border: 1px solid #999;
    border-radius: 6px;
}

/* Checkboxes and Radio Buttons */
QCheckBox::indicator, QRadioButton::indicator {
    border: 1px solid #666;
    background: #ffffff;
}

QCheckBox::indicator:checked, QRadioButton::indicator:checked {
    background-color: #ffc107;
}

QCheckBox::indicator:hover, QRadioButton::indicator:hover {
    border: 1px solid #1976d2;
}
